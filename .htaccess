
Options +FollowSymLinks

RewriteEngine On
RewriteCond %{THE_REQUEST} /index.html HTTP [NC]
RewriteRule (.*)index.html$ /$1 [R=301,L]

RewriteCond %{THE_REQUEST} /index#about-me HTTP [NC]
RewriteRule (.*)index#about-me$ /about$1 [R=301,L]

RewriteCond %{THE_REQUEST} /index#projects HTTP [NC]
RewriteRule (.*)index#projects$ /projects$1 [R=301,L]

RewriteCond %{THE_REQUEST} /index#contact HTTP [NC]
RewriteRule (.*)index#contact$ /contact$1 [R=301,L]



RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]